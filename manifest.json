{
  "manifest_version": 3,
  "name": "Share",
  "version": "1.0.0",
  "permissions": [
    "activeTab",
    "scripting",
    "contextMenus"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon-light-16.png",
      "24": "icons/icon-light-24.png",
      "32": "icons/icon-light-32.png",
      "48": "icons/icon-light-48.png",
      "64": "icons/icon-light-64.png",
      "128": "icons/icon-light-128.png"
    },
    "default_title": "Share"
  }
}
